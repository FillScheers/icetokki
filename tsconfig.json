{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@icetokki/api-interfaces": ["libs/api-interfaces/src/index.ts"],
      "@icetokki/playlist/domain": ["libs/playlist/domain/src/index.ts"],
      "@icetokki/playlist/feature-create": [
        "libs/playlist/feature-create/src/index.ts"
      ],
      "@icetokki/playlist/feature-manage": [
        "libs/playlist/feature-manage/src/index.ts"
      ]
    }
  },
  "angularCompilerOptions": {
    "strictTemplates": true,
    "strictInjectionParameters": true
  },
  "exclude": ["node_modules", "tmp"],
  "files": [],
  "references": [
    {
      "path": "./apps/tokkify/tsconfig.app.json"
    },
    {
      "path": "./apps/tokkify/tsconfig.spec.json"
    },
    {
      "path": "./apps/tokkify-e2e/tsconfig.e2e.json"
    },
    {
      "path": "./apps/api/tsconfig.app.json"
    },
    {
      "path": "./apps/api/tsconfig.spec.json"
    },
    {
      "path": "./libs/api-interfaces/tsconfig.spec.json"
    },
    {
      "path": "./player-domain/tsconfig.lib.json"
    },
    {
      "path": "./player-domain/tsconfig.spec.json"
    },
    {
      "path": "./player-feature-player/tsconfig.lib.json"
    },
    {
      "path": "./player-feature-player/tsconfig.spec.json"
    },
    {
      "path": "./playlist-domain/tsconfig.lib.json"
    },
    {
      "path": "./playlist-domain/tsconfig.spec.json"
    },
    {
      "path": "./playlist-feature-create/tsconfig.lib.json"
    },
    {
      "path": "./playlist-feature-create/tsconfig.spec.json"
    },
    {
      "path": "./playlist-feature-manage/tsconfig.lib.json"
    },
    {
      "path": "./playlist-feature-manage/tsconfig.spec.json"
    }
  ]
}
